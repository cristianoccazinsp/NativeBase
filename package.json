    "fs-extra": "^2.0.0",
    "hoist-non-react-statics": "^3.0.0",
    "jest-react-native": "^18.0.0",
    "lodash": "^4.17.21",
    "print-message": "^2.1.0",
    "prop-types": "^15.5.10",
    "react-native-easy-grid": "0.2.2",
    "react-native-vector-icons": "^8.1.0",
    "react-timer-mixin": "^0.13.4"
  },
  "devDependencies": {
